{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "object-curly-spacing": [
        2,
        "never"
      ],
      "computed-property-spacing": [
        2,
        "never"
      ],
      "react/prop-types": "off"
    }
}
